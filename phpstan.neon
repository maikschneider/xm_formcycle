includes:
   - vendor/saschaegerer/phpstan-typo3/extension.neon
   - phpstan-baseline.neon

parameters:
    parallel:
        maximumNumberOfProcesses: 5

    level: 5

    bootstrapFiles:
        - vendor/autoload.php

    paths:
        - .

    excludePaths:
        - vendor
        - var
        - php-cs-fixer.php
        - ext_emconf.php
